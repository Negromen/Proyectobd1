                    <label for="inputLaboratorio" class="form-label">Laboratorio</label>
                    <input type="text" class="form-control" id="inputLaboratorio" placeholder="Laboratorio">
                </div>
                <div class="col-md-4">
                    <label for="idNombreVacuna" class="form-label">Nombre de Vacuna</label>
                    <input type="text" class="form-control" id="idNombreVacuna" placeholder="Vacuna">
                </div>
                <div class="col-md-3">
